{
  "expo": {
    "name": "AgriHubDavao",
    "slug": "agrihub-davao",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/agrihub-davao-logo.png",
    "scheme": "AgriHubDavao",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/agrihub-desktop-logo.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.deive03.agrihubdavao",
      "windowSoftInputMode": "adjustResize",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "CAMERA"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD7qx0WjaWjvtt3XMaBoN_zAx4y80Dnrhg"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "react-native-permissions"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "firebase": {
        "apiKey": "AIzaSyCCfQx8Ru9AgZCcJKeSUlGoxM8Y74ApLKg",
        "authDomain": "manota-f9acc.firebaseapp.com",
        "projectId": "manota-f9acc",
        "storageBucket": "manota-f9acc.firebasestorage.app",
        "messagingSenderId": "44603281991",
        "appId": "1:44603281991:web:dc52f67beaaaff11c59762",
        "databaseURL": "https://manota-f9acc-default-rtdb.firebaseio.com/"
      },
      "eas": {
        "projectId": "b593a07f-9c26-44af-8ae6-70882f1b8fef"
      }
    }
  }
}
